{
  "name": "@thepiedpiper/infra-xyz",
  "description": "infra-xyz is a tool for bootstraping the infra required to run the piper-pied in all deploys be it production, staging, testing, or development.",
  "version": "0.1.0",
  "author": "thepiperpied",
  "bin": {
    "xyz": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/thepiperpied/infra-xyz/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "execa": "^4.0.0",
    "kleur": "^3.0.3",
    "lowdb": "^1.0.0",
    "taskz": "^1.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.8",
    "@types/lowdb": "^1.0.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.14",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/thepiperpied/infra-xyz#readme",
  "keywords": [],
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "xyz",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thepiperpied/infra-xyz.git"
  },
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "ci": "npm run prepack",
    "build": "npm run ci"
  },
  "types": "lib/index.d.ts"
}
